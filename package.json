{
  "name": "linkShortner",
  "version": "1.0.0",
  "private": true,
  "description": "Shorten 'em links dawg!",
  "scripts": {
    "build": "./node_modules/.bin/browserify ./public/javascripts/main.js | ./node_modules/.bin/uglifyjs -c -m > ./public/javascripts/main.min.js",
    "start": "DEBUG=linkShortner,linkShortner:* nodemon ./bin/www"
  },
  "dependencies": {
    "base62": "^1.1.0",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "^2.1.3",
    "ejs": "~2.3.1",
    "express": "~4.12.2",
    "flatfile": "^0.1.1",
    "jquery": "^2.1.3",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "nodemon": "^1.3.7",
    "uglify-js": "^2.4.19"
  }
}
